load res.mat
totalSize = length(res(:, 1));
SLIDE = 1;
DRAG = 2;
CLICK = 3;
FORCE_CLICK = 4;
UNKNOWN = 5;

slide = res((res(:, 3) == SLIDE | res(:, 3) == DRAG) == 1, 1:2);
click = res((res(:, 3) == CLICK | res(:, 3) == FORCE_CLICK) == 1, 1:2);

slide = slide((res(:, 1) > 0 & (res(:, 2) < 0)) == 1, 1:2); = [];
clickRatio = [];

slide = slide((res(:, 1) > 0 & (res(:, 2) < 0)) == 1, 1:2);
click = click((res(:, 1) > 0 & (res(:, 2) < 0)) == 1, 1:2);